                    </section>
                    <script type="text/javascript" language="javascript">
                        $(function() {
                            $('#changevehiclebtn').click(function() {
                                $('#displayvehicle').hide();
                                $('#changevehicle').show();
                            });
                            $('.imgdialog').click(function() {
                                var html = '';
                                if($(this).next(".addl_views").length) {
                                    html = $(this).next().html();
                                } else {
                                    var image = ($this).attr('src').slice(0,-5)+"500/500";
                                    html ='<img id="zoom" src="'+image+'" />';
                                }
                                $dialog.dialog('open').html(html);
                                return false;
                            });
                            var $dialog = $('<div></div>')
                            .dialog({
                                autoOpen: false,
                                modal: true,
                                width: 540,
                                height: 660,
                                title: '<?php echo $this->productLine->getDisplayName(); ?>'
                            });
                            $('.ui-dialog').on('click', '.addlminiimg', function(){
                                var addlimage = $(this).attr('src').slice(0,-5)+"500/500";
                                $('.ui-dialog-content #zoom').attr('src',addlimage);
                                return false;
                            });
                            var loginOpts = {
                                title: 'Submit Your Review',
                                modal: true,
                                autoOpen: true,
                                draggable: false,
                                width: 540,
                                height: 590
                            }
                            $('#loginreviewbtn').on('click', function(){
                                $('#login_review').dialog(loginOpts);
                                return false;
                            });
                            $('#loginsubmitbtn').on('click', function(e){
                                e.preventDefault();

                                $.ajax({
                                    url: '/login',
                                    type: 'POST',
                                    dataType: 'json',
                                    data: $('#login-form').serialize(),
                                    success: function(res){
                                        if (res.error) {
                                          $('#authformerror').show();
                                        } else {
                                          $('#authforms').hide();
                                          $('#reviewloginlink').hide();
                                          $('#reviewform').show();
                                          $('#reviewhellolink').show();
                                          $('#reviewername').html(res.data.firstName);
                                          $('#revieweruserid').val(res.data.userId);
                                        }
                                    }
                                });
                            });
                            $('#loggedinreviewbtn').on('click', function(){
                                $('#authforms').hide();
                                $('#reviewform').show();
                                $('#login_review').dialog(loginOpts);
                                return false;
                            });
                            $('#registersubmitbtn').on('click', function(e){
                                e.preventDefault();
                                $('input[name="user-fieldset[username]"]').val($('input[name="user-fieldset[emailAddress]"]').val());

                                $.ajax({
                                    url: '/products/register',
                                    type: 'POST',
                                    dataType: 'json',
                                    data: $('#create-user-form').serialize(),
                                    success: function(res){
                                        if (res.data.error) {
                                            $('#registererror').html(res.data.error);
                                        } else {
                                            $('#authforms').hide();
                                            $('#reviewloginlink').hide();
                                            $('#reviewform').show();
                                            $('#reviewhellolink').show();
                                            $('#reviewername').html(res.data.firstName);
                                            $('#revieweruserid').val(res.data.userId);
                                        }
                                    }
                                });
                            });
                            $('#reviewsubmitbtn').on('click', function(e){
                                e.preventDefault();

                                $.ajax({
                                    url: '/products/review',
                                    type: 'POST',
                                    dataType: 'json',
                                    data: $('#product-review-form').serialize(),
                                    success: function(res){
                                        if (res.data.error) {
                                            $('#reviewerror').html(res.data.error);
                                            $('#reviewerror').show();
                                        } else {
                                            $('#reviewerror').html('Thank you! You have successfully submitted your review.');
                                            $('#reviewerror').show();
                                            $('#product-review-form').hide();
                                        }
                                    }
                                });
                            });
                        });
                    </script>
