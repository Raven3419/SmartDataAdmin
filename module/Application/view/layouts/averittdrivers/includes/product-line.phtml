                                <input type="hidden" name="product-review-fieldset[productLines]" value="<?php echo $this->productLine->getProductLineId(); ?>" />
                                <input type="hidden" id="revieweruserid" name="product-review-fieldset[user]" value="<?php echo ($this->identity() ? $this->identity()->getUserId() : ''); ?>" />
                                <?php echo $this->formElement($reviewForm->get('secret')); ?>
                                <input type="hidden" name="product-review-fieldset[disabled]" value="1" />
                                <?php $rating = $reviewFieldset->get('rating'); ?>
                                <label><?php echo $rating->getLabel(); ?></label>
                                <?php echo $this->formElement($rating); ?>
                                <?php $review = $reviewFieldset->get('review'); ?>
                                <label><?php echo $review->getLabel(); ?></label>
